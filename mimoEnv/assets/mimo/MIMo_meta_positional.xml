<!-- This file is intended to allow for posing of MIMo into static positions. -->
<mujoco model="MIMo">

	<default>
		<default class="mimo">
        	<joint limited="true" damping="1" stiffness="1" armature="0.01" solimplimit="0.98 0.99 0.001 .5 1" solreflimit="0.02 1"/>
        	<geom condim="3" material="skin" friction="1 0.005 0.0002" solimp="0.95 0.99 0.001 0.5 1" solref="0.005 1"/>
        	<motor ctrlrange="-1 1" ctrllimited="true" forcelimited="true"/>
		</default>
    </default>

    <asset>

		<texture name="tex_head_default" type="cube" fileright="tex/head_front.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_happy" type="cube" fileright="tex/head_front_happy.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_sad" type="cube" fileright="tex/head_front_sad.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_surprised" type="cube" fileright="tex/head_front_surprised.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_angry" type="cube" fileright="tex/head_front_angry.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_disgusted" type="cube" fileright="tex/head_front_disgusted.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_head_scared" type="cube" fileright="tex/head_front_scared.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_top_sleeve" type="cube" fileright="tex/sleeve_right.png" fileleft="tex/sleeve_left.png" fileup="tex/sleeve_up.png" filedown="tex/sleeve_down.png" filefront="tex/sleeve_front.png" fileback="tex/sleeve_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_pants_leg" type="cube" fileright="tex/leg_right.png" fileleft="tex/leg_left.png" fileup="tex/leg_up.png" filedown="tex/leg_down.png" filefront="tex/leg_front.png" fileback="tex/leg_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_eye_left" type="cube" fileright="tex/eye_pupil_left.png" rgb1="1 1 1"/>
		<texture name="tex_eye_right" type="cube" fileright="tex/eye_pupil_right.png" rgb1="1 1 1"/>

		<material name="skin" rgba="0.95 0.8 0.5 1" specular=".15" shininess=".7"/>
		<material name="buttons" rgba=".05 .05 .05 1" specular=".9" shininess=".9"/>
		<material name="head" texture="tex_head_default" texuniform="true" specular=".0" shininess=".0"/>
		<material name="top_up" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_middle" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_down" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_sleeve" texture="tex_top_sleeve" texuniform="true" specular=".0" shininess=".0"/>
		<material name="pants_up" rgba="0.1 0.1 0.6 1" specular=".0" shininess=".0"/>
		<material name="pants_down" rgba="0.1 0.1 0.6 1" specular=".0" shininess=".0"/>
		<material name="pants_leg" texture="tex_pants_leg" texuniform="true" specular=".0" shininess=".0"/>
		<material name="eye_left" texture="tex_eye_left" texuniform="true" specular=".0" shininess=".0"/>
		<material name="eye_right" texture="tex_eye_right" texuniform="true" specular=".0" shininess=".0"/>

    </asset>
	
	<!-- Internal contacts should not have friction to reduce "getting stuck" on ourselves-->
	<contact>
		<pair geom1="lb" geom2="ub1" condim="1"/>
		<exclude body1="left_eye" body2="upper_body"/>
		<exclude body1="right_eye" body2="upper_body"/>
	</contact>
	
	<tendon>
		<fixed class="mimo" name="hip_bend" limited="true" range="-.01 .01">
			<joint joint="robot:hip_bend1" coef="1"/>
			<joint joint="robot:hip_bend2" coef="-1"/>
		</fixed>
		<fixed class="mimo" name="hip_rotation" limited="true" range="-.01 .01">
			<joint joint="robot:hip_rot1" coef="1"/>
			<joint joint="robot:hip_rot2" coef="-1"/>
		</fixed>
		<fixed class="mimo" name="hip_lean" limited="true" range="-.01 .01">
			<joint joint="robot:hip_lean1" coef="1"/>
			<joint joint="robot:hip_lean2" coef="-.5"/>
		</fixed>
		<fixed class="mimo" name="head_limits1" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="1"/>
		</fixed>
		<fixed class="mimo" name="head_limits2" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_tilt" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="1"/>
		</fixed>
		<fixed class="mimo" name="head_limits3" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt" coef="1"/>
		</fixed>
		<fixed class="mimo" name="head_limits1_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="-1"/>
		</fixed>
		<fixed class="mimo" name="head_limits2_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_tilt" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="-1"/>
		</fixed>
		<fixed class="mimo" name="head_limits3_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt" coef="-1"/>
		</fixed>
	</tendon>
	
	<sensor>
		<accelerometer name="vestibular_acc" site="vestibular"/>
		<gyro name="vestibular_gyro" site="vestibular"/>
		
		<torque name="proprio:neck" site="torque_neck"/>
		<torque name="proprio:left_eye" site="torque_left_eye"/>
		<torque name="proprio:right_eye" site="torque_right_eye"/>
		
		<torque name="proprio:trunk_upper" site="torque_ub"/>
		<torque name="proprio:trunk_lower" site="torque_lb"/>
		
		<torque name="proprio:right_shoulder" site="torque_right_shoulder"/>
		<torque name="proprio:right_elbow" site="torque_right_elbow"/>
		<torque name="proprio:right_wrist" site="torque_right_wrist"/>
		<torque name="proprio:right_fingers" site="torque_right_fingers"/>
		
		<torque name="proprio:left_shoulder" site="torque_left_shoulder"/>
		<torque name="proprio:left_elbow" site="torque_left_elbow"/>
		<torque name="proprio:left_wrist" site="torque_left_wrist"/>
		<torque name="proprio:left_fingers" site="torque_left_fingers"/>
		
		<torque name="proprio:right_hip" site="torque_right_hip"/>
		<torque name="proprio:right_knee" site="torque_right_knee"/>
		<torque name="proprio:right_ankle" site="torque_right_ankle"/>
		<torque name="proprio:right_toes" site="torque_right_toes"/>
		
		<torque name="proprio:left_hip" site="torque_left_hip"/>
		<torque name="proprio:left_knee" site="torque_left_knee"/>
		<torque name="proprio:left_ankle" site="torque_left_ankle"/>
		<torque name="proprio:left_toes" site="torque_left_toes"/>
	</sensor>

	<actuator>

		<position class="mimo" name="act:hip_bend" gear="57.296" kp=".5" joint="robot:hip_bend1" ctrlrange="-17 30.5" forcelimited="true" forcerange="-.4 .4"/>
		<position class="mimo" name="act:hip_twist" gear="57.296" kp=".3" joint="robot:hip_rot1" ctrlrange="-18 18" forcelimited="true" forcerange="-.2 .2"/>
		<position class="mimo" name="act:hip_lean" gear="57.296" kp=".5" joint="robot:hip_lean1" ctrlrange="-14 14" forcelimited="true" forcerange="-.4 .4"/>
		
		<position class="mimo" name="act:head_swivel" gear="57.296" kp=".3" joint="robot:head_swivel" ctrlrange="-111 111" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:head_tilt" gear="57.296" kp=".3" joint="robot:head_tilt" ctrlrange="-70 81" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:head_tilt_side" gear="57.296" kp=".3" joint="robot:head_tilt_side" ctrlrange="-70 70" forcelimited="true" forcerange="-.03 .03"/>

		<position class="mimo" name="act:left_eye_horizontal" gear="57.296" kp=".03" joint="robot:left_eye_horizontal" ctrlrange="-45 45" forcelimited="true" forcerange="-.00002 .00002"/>
		<position class="mimo" name="act:left_eye_vertical" gear="57.296" kp=".03" joint="robot:left_eye_vertical" ctrlrange="-47 33" forcelimited="true" forcerange="-.00002 .00002"/>
		<position class="mimo" name="act:left_eye_torsional" gear="57.296" kp=".03" joint="robot:left_eye_torsional" ctrlrange="-8 8" forcelimited="true" forcerange="-.00002 .00002"/>

		<position class="mimo" name="act:right_eye_horizontal" gear="57.296" kp=".03" joint="robot:right_eye_horizontal" ctrlrange="-45 45" forcelimited="true" forcerange="-.00002 .00002"/>
		<position class="mimo" name="act:right_eye_vertical" gear="57.296" kp=".03" joint="robot:right_eye_vertical" ctrlrange="-47 33" forcelimited="true" forcerange="-.00002 .00002"/>
		<position class="mimo" name="act:right_eye_torsional" gear="57.296" kp=".03" joint="robot:right_eye_torsional" ctrlrange="-8 8" forcelimited="true" forcerange="-.00002 .00002"/>

		<position class="mimo" name="act:right_shoulder1" gear="57.296" kp=".005" joint="robot:right_shoulder_horizontal" ctrlrange="-28 118" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:right_shoulder2" gear="57.296" kp=".005" joint="robot:right_shoulder_ad_ab" ctrlrange="-84 183" forcelimited="true" forcerange="-.06 .06"/>
		<position class="mimo" name="act:right_shoulder3" gear="57.296" kp=".005" joint="robot:right_shoulder_rotation" ctrlrange="-99 67" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:right_elbow" gear="57.296" kp=".01" joint="robot:right_elbow" ctrlrange="-146 5" forcelimited="true" forcerange="-.06 .06"/>
		<position class="mimo" name="act:right_hand1" gear="57.296" kp=".005" joint="robot:right_hand1" ctrlrange="-90 90" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:right_hand2" gear="57.296" kp=".005" joint="robot:right_hand2" ctrlrange="-92 86" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:right_hand3" gear="57.296" kp=".005" joint="robot:right_hand3" ctrlrange="-53 48" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:right_fingers" gear="57.296" kp=".005" joint="robot:right_fingers" ctrlrange="-160 8" forcelimited="true" forcerange="-.005 .005"/>

		<position class="mimo" name="act:left_shoulder1" gear="57.296" kp=".005" joint="robot:left_shoulder_horizontal" ctrlrange="-28 118" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:left_shoulder2" gear="57.296" kp=".005" joint="robot:left_shoulder_ad_ab" ctrlrange="-84 183" forcelimited="true" forcerange="-.06 .06"/>
		<position class="mimo" name="act:left_shoulder3" gear="57.296" kp=".005" joint="robot:left_shoulder_rotation" ctrlrange="-99 67" forcelimited="true" forcerange="-.03 .03"/>
		<position class="mimo" name="act:left_elbow" gear="57.296" kp=".01" joint="robot:left_elbow" ctrlrange="-146 5" forcelimited="true" forcerange="-.06 .06"/>
		<position class="mimo" name="act:left_hand1" gear="57.296" kp=".005" joint="robot:left_hand1" ctrlrange="-90 90" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:left_hand2" gear="57.296" kp=".005" joint="robot:left_hand2" ctrlrange="-92 86" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:left_hand3" gear="57.296" kp=".005" joint="robot:left_hand3" ctrlrange="-53 48" forcelimited="true" forcerange="-.02 .02"/>
		<position class="mimo" name="act:left_fingers" gear="57.296" kp=".005" joint="robot:left_fingers" ctrlrange="-160 8" forcelimited="true" forcerange="-.005 .005"/>

		<position class="mimo" name="act:right_hip1" gear="57.296" kp=".1" joint="robot:right_hip1" ctrlrange="-133 20" forcelimited="true" forcerange="-.2 .2"/>
		<position class="mimo" name="act:right_hip2" gear="57.296" kp=".1" joint="robot:right_hip2" ctrlrange="-51 17" forcelimited="true" forcerange="-0.175 0.175"/>
		<position class="mimo" name="act:right_hip3" gear="57.296" kp=".1" joint="robot:right_hip3" ctrlrange="-32 41" forcelimited="true" forcerange="-0.1 0.1"/>
		<position class="mimo" name="act:right_knee" gear="57.296" kp=".01" joint="robot:right_knee" ctrlrange="-145 4" forcelimited="true" forcerange="-0.2 0.2"/>
		<position class="mimo" name="act:right_foot1" gear="57.296" kp=".01" joint="robot:right_foot1" ctrlrange="-63 32" forcelimited="true" forcerange="-0.02 0.02"/>
		<position class="mimo" name="act:right_foot2" gear="57.296" kp=".01" joint="robot:right_foot2" ctrlrange="-33 31" forcelimited="true" forcerange="-0.02 0.02"/>
		<position class="mimo" name="act:right_foot3" gear="57.296" kp=".01" joint="robot:right_foot3" ctrlrange="-20 30" forcelimited="true" forcerange="-0.02 0.02"/>
		<position class="mimo" name="act:right_toes" gear="57.296" kp=".01" joint="robot:right_toes" ctrlrange="-60 80" forcelimited="true" forcerange="-0.01 0.01"/>

