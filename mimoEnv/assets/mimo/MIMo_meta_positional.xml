		<position class="mimo" name="act:left_foot3" gear="57.296" kp=".01" joint="robot:left_foot3" ctrlrange="-20 30" forcelimited="true" forcerange="-0.02 0.02"/>
		<position class="mimo" name="act:left_toes" gear="57.296" kp=".01" joint="robot:left_toes" ctrlrange="-60 80" forcelimited="true" forcerange="-0.01 0.01"/>

